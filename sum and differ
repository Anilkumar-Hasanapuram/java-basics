import java.io.*;
import java.util.Scanner;
public class SumDiff
{
    public static void main(String args[])
    {
        //write your code
        Scanner sc=new Scanner(System.in);
        int a,b;
        a=sc.nextInt();
        b=sc.nextInt();
        System.out.println(a+b);
        System.out.println(a-b);
    }
}
